{
  "expo": {
    "name": "SocialApp",
    "slug": "SocialApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "config":{
        "googleSignIn":{
          "reservedClientId": "com.googleusercontent.apps.826391558870-8pqge7id1v6h2i7q6e4u4juq3clmse6v"
        }
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.fmurillo.SocialApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.fmurillo.SocialApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
